<script type="text/javascript">
    $(function () {
        $("body").Huitab({
            tabBar: ".navbar-wrapper .navbar-levelone",
            tabCon: ".Hui-aside .menu_dropdown",
            className: "current",
            index: 0,
        });
    });
    function frameOpen(url) {
        $("#main_iframe").prop("src", url);
    }
    //检查并使iframe跳转
    function frameCheckOpen(checkType, url) {
        //检查是否选择的非0
        if (checkType == 1) {
            if ($("#metaObjectSelect").val() == 0) {
                alert("请先选择对象!");
                return;
            }
        }
        //checktype = x 其他类型检查
        frameOpen(url);
    }
    function metaObjectSwitch(obj) {
        var selected = $(obj).val();
        if (selected == 0) {
            frameOpen('/MetaObject/List');
        } else {
            frameOpen('/MetaObject/Switch/' + $(obj).val());
        }
    }
</script>
<!--/请在上方写此页面业务相关的脚本-->